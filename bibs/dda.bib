@phdthesis{yeung:mcr1991,
author= {M-C. R. Yeung},
title= {Application of {S}hi's {D}iscontinuous {D}eformation
{A}nalysis to the {S}tudy of {R}ock {B}ehavior},
school = {University of California, Berkeley},
year=1991,
comment ={Yeung investigated the mechanics of a
single block on an incline, numerically solved an equation of
motion for a three hinged beam and investigated rock bolt
interaction in mine roofs.  The single block model showed that
dynamic equilibrium should be considered in the mode of behavior
of a block system, that is, whether sliding or sliding and
toppling occur.  The three hinged beam problem showed that
DDA can be dependent on the size of the time step, larger time
steps proved unconservative.  },
}
